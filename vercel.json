{
  "version": 2,
  "public": true,
  "name": "e-commerce",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/(.*)", "destination": "/api" },
    { "source": "/(.*)", "destination": "/" }
  ],
  "functions": {
    "api/index.js": {
      "includeFiles": "dist/e-commerce/**"
    }
  }
}
